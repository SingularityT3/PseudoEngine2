PRINT "LEGNTH(\"ABCDEFG\"): " & LENGTH("ABCDEFG")
PRINT "LEFT(\"ABCDEFG\", 3): " & LEFT("ABCDEFG", 3)
PRINT "RIGHT(\"ABCDEFG\", 2): " & RIGHT("ABCDEFG", 2)
PRINT "MID(\"ABCDEFG\", 4, 2): " & MID("ABCDEFG", 4, 2)
PRINT "TO_UPPER(\"ThiS 1s a T3st 112!\"): " & TO_UPPER("ThiS 1s a T3st!")
PRINT "TO_LOWER(\"ThiS 1s a T3st 112!\"): " & TO_LOWER("ThiS 1s a T3st!")

PRINT "NUM_TO_STR(245.78): " & NUM_TO_STR(245.78)
PRINT "STR_TO_NUM(\"56\"): " & STR_TO_NUM("56")
PRINT "IS_NUM(\"2.412\"): " & IS_NUM("2.412")
PRINT "IS_NUM(\"2.412.13\"): " & IS_NUM("2.412.13")
PRINT "IS_NUM(\"nan\"): " & IS_NUM("nan")

PRINT "LCASE('W'): " & LCASE('W')
PRINT "LCASE('a'): " & LCASE('a')
PRINT "UCASE('s'): " & UCASE('s')
PRINT "UCASE('D'): " & UCASE('D')
PRINT "ASC('A'): " & ASC('A')
PRINT "CHR(99): " & CHR(99)

PRINT "INT(92.235): " & INT(92.235)
PRINT "RAND(1011): " & RAND(1011)

PRINT "POW(8, 2/3): " & POW(8, 2 / 3.0)
PRINT "EXP(LN(2)): " & EXP(LN(2))

CONSTANT PI = 3.141592654

PRINT "SIN(pi/6): " & SIN(PI / 6)
PRINT "COS(pi/3): " & COS(PI / 3)
PRINT "TAN(PI / 4): " & TAN(PI / 4)

PRINT "ASIN(SQRT(3) / 2) / PI: " & ASIN(SQRT(3) / 2) / PI
PRINT "ACOS(0.5) / PI: " & ACOS(0.5) / PI
PRINT "ATAN(5): " & ATAN(5)
PRINT "ATAN2(5, 12): " & ATAN2(5, 12)

PRINT "LOG(100): " & LOG(100)
PRINT "LN(e): " & LN(EXP(1))
